{
    "id": "tools.moimz.imodules.modules.member",
    "icon": "xi xi-users",
    "title": {
        "ko": "회원"
    },
    "version": "4.0.0",
    "description": {
        "ko": "사이트 통합 회원관리를 위한 모듈입니다."
    },
    "author": {
        "name": "Arzz",
        "email": "arzz@arzz.com"
    },
    "homepage": "https://www.imodules.io",
    "versionChecker": "https://api.moimz.com/download/latest",
    "dependencies": {
        "core": "4.0.0"
    },
    "language": "ko",
    "global": true,
    "admin": true,
    "context": true,
    "configs": {
        "template": {
            "type": "template",
            "label": {
                "ko": "템플릿"
            },
            "component": {
                "type": "module",
                "name": "member"
            },
            "default": "default"
        }
    },
    "databases": {
        "members": {
            "columns": {
                "member_id": {
                    "type": "int",
                    "comment": "회원고유값"
                },
                "email": {
                    "type": "varchar",
                    "length": 100,
                    "comment": "이메일"
                },
                "password": {
                    "type": "char",
                    "length": 65,
                    "is_null": true,
                    "comment": "패스워드"
                },
                "level_id": {
                    "type": "int",
                    "default": 0,
                    "comment": "레벨고유값"
                },
                "code": {
                    "type": "varchar",
                    "length": 50,
                    "is_null": true,
                    "comment": "회원추가코드"
                },
                "name": {
                    "type": "varchar",
                    "length": 50,
                    "is_null": true,
                    "comment": "이름"
                },
                "nickname": {
                    "type": "varchar",
                    "length": 20,
                    "comment": "닉네임"
                },
                "homepage": {
                    "type": "varchar",
                    "length": 100,
                    "is_null": true,
                    "comment": "홈페이지"
                },
                "telephone": {
                    "type": "varchar",
                    "length": 20,
                    "is_null": true,
                    "comment": "전화번호"
                },
                "cellphone": {
                    "type": "varchar",
                    "length": 20,
                    "is_null": true,
                    "comment": "휴대전화번호"
                },
                "gender": {
                    "type": "enum",
                    "length": "'MALE','FEMALE'",
                    "is_null": true,
                    "comment": "성별"
                },
                "birthday": {
                    "type": "char",
                    "length": 10,
                    "is_null": true,
                    "comment": "생일(월-일-년)"
                },
                "zipcode": {
                    "type": "varchar",
                    "length": 10,
                    "is_null": true,
                    "comment": "우편번호"
                },
                "address": {
                    "type": "text",
                    "is_null": true,
                    "comment": "주소"
                },
                "joined_at": {
                    "type": "int",
                    "comment": "가입일시"
                },
                "leaved_at": {
                    "type": "int",
                    "is_null": true,
                    "comment": "탈퇴일시"
                },
                "logged_at": {
                    "type": "int",
                    "is_null": true,
                    "comment": "최종로그인일시"
                },
                "logged_ip": {
                    "type": "varchar",
                    "length": 39,
                    "is_null": true,
                    "comment": "최종로그인IP"
                },
                "verified": {
                    "type": "enum",
                    "length": "'TRUE','FALSE'",
                    "default": "FALSE",
                    "comment": "인증여부"
                },
                "status": {
                    "type": "enum",
                    "length": "'ACTIVATED','WAITING','LEAVE','DEACTIVATED'",
                    "default": "ACTIVATED",
                    "comment": "상태"
                },
                "extras": {
                    "type": "json",
                    "is_null": true,
                    "comment": "추가정보"
                }
            },
            "indexes": {
                "member_id": "primary_key",
                "email": "unique",
                "code": "index",
                "level_id": "index"
            },
            "auto_increment": "member_id",
            "comment": "아이모듈 회원"
        },
        "groups": {
            "columns": {
                "group_id": {
                    "type": "varchar",
                    "length": 36,
                    "comment": "그룹고유값"
                },
                "parent_id": {
                    "type": "char",
                    "length": 36,
                    "is_null": true,
                    "comment": "상위그룹고유값"
                },
                "depth": {
                    "type": "int",
                    "default": 0,
                    "comment": "단계"
                },
                "title": {
                    "type": "varchar",
                    "length": 30,
                    "comment": "그룹명"
                },
                "members": {
                    "type": "int",
                    "default": 0,
                    "comment": "인원수"
                }
            },
            "indexes": {
                "group_id": "primary_key",
                "parent_id": "index"
            },
            "comment": "아이모듈 회원 그룹"
        },
        "group_members": {
            "columns": {
                "group_id": {
                    "type": "char",
                    "length": 36,
                    "comment": "그룹고유값"
                },
                "member_id": {
                    "type": "int",
                    "length": 11,
                    "comment": "회원고유값"
                },
                "assigned_at": {
                    "type": "int",
                    "comment": "그룹할당일시"
                }
            },
            "indexes": {
                "group_id,member_id": "primary_key",
                "group_id": "index",
                "member_id": "index"
            },
            "comment": "아이모듈 회원 그룹 소속회원"
        },
        "levels": {
            "columns": {
                "level_id": {
                    "type": "int",
                    "comment": "레벨고유값"
                },
                "title": {
                    "type": "varchar",
                    "length": 30,
                    "comment": "레벨명"
                },
                "members": {
                    "type": "int",
                    "default": 0,
                    "comment": "인원수"
                }
            },
            "indexes": {
                "level_id": "primary_key"
            },
            "comment": "아이모듈 회원레벨"
        },
        "logins": {
            "columns": {
                "login_id": {
                    "type": "char",
                    "length": 36,
                    "comment": "자동로그인 고유값"
                },
                "member_id": {
                    "type": "int",
                    "comment": "회원고유값"
                },
                "created_at": {
                    "type": "int",
                    "comment": "자동로그인 생성일시"
                },
                "created_ip": {
                    "type": "varchar",
                    "length": 39,
                    "comment": "자동로그인 생성IP"
                },
                "logged_at": {
                    "type": "int",
                    "comment": "최종로그인일시"
                },
                "logged_ip": {
                    "type": "varchar",
                    "length": 39,
                    "is_null": true,
                    "comment": "최종로그인IP"
                },
                "agent": {
                    "type": "varchar",
                    "length": 255,
                    "comment": "접속디바이스"
                }
            },
            "indexes": {
                "login_id": "primary_key",
                "member_id": "index"
            },
            "comment": "아이모듈 회원 자동로그인"
        },
        "logs": {
            "columns": {
                "time": {
                    "type": "double",
                    "comment": "기록일시(마이크로타임)"
                },
                "member_id": {
                    "type": "int",
                    "comment": "회원고유값"
                },
                "component_type": {
                    "type": "varchar",
                    "length": 10,
                    "comment": "기록이 발생한 컴포넌트종류"
                },
                "component_name": {
                    "type": "varchar",
                    "length": 50,
                    "comment": "기록이 발생한 컴포넌트명"
                },
                "log_type": {
                    "type": "varchar",
                    "length": 50,
                    "comment": "기록종류"
                },
                "log_id": {
                    "type": "varchar",
                    "length": 50,
                    "comment": "기록고유값"
                },
                "details": {
                    "type": "longtext",
                    "is_null": true,
                    "comment": "상세기록"
                },
                "ip": {
                    "type": "varchar",
                    "length": 39,
                    "comment": "접근IP"
                },
                "agent": {
                    "type": "varchar",
                    "length": 255,
                    "comment": "접속디바이스"
                }
            },
            "indexes": {
                "time,member_id": "primary_key",
                "member_id": "index",
                "component_type,component_name": "index",
                "ip": "index"
            },
            "comment": "아이모듈 회원 기록"
        },
        "oauth_clients": {
            "columns": {
                "oauth_id": {
                    "type": "varchar",
                    "length": 20,
                    "comment": "제공자고유값"
                },
                "client_id": {
                    "type": "varchar",
                    "length": 255,
                    "comment": "OAuth client_id"
                },
                "client_secret": {
                    "type": "varchar",
                    "length": 255,
                    "comment": "OAuth client_secret"
                },
                "auth_url": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "인증요청 URL"
                },
                "token_url": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "토큰요청 URL"
                },
                "scope": {
                    "type": "text",
                    "is_null": true,
                    "comment": "OAuth 요청범위"
                },
                "scope_separator": {
                    "type": "enum",
                    "length": "'COMMA','SPACE','COLONS','SEMICOLONS','PLUS'",
                    "default": "COMMA",
                    "comment": "OAuth 범위구분자"
                },
                "user_url": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "유저정보요청 URL"
                },
                "user_id_path": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "유저고유값경로"
                },
                "user_email_path": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "유저이메일경로"
                },
                "user_name_path": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "유저이름경로"
                },
                "user_nickname_path": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "유저닉네임경로"
                },
                "user_photo_path": {
                    "type": "varchar",
                    "length": 200,
                    "is_null": true,
                    "comment": "유저프로필이미지경로"
                },
                "allow_signup": {
                    "type": "enum",
                    "length": "'TRUE','FALSE'",
                    "default": "TRUE",
                    "comment": "회원가입허용여부"
                },
                "sort": {
                    "type": "int",
                    "length": 11,
                    "default": 0,
                    "comment": "정렬"
                }
            },
            "indexes": {
                "oauth_id": "primary_key"
            },
            "comment": "아이모듈 OAuth 클라이언트"
        },
        "oauth_tokens": {
            "columns": {
                "oauth_id": {
                    "type": "varchar",
                    "length": 20,
                    "comment": "제공자고유값"
                },
                "member_id": {
                    "type": "int",
                    "length": 11,
                    "comment": "회원고유값"
                },
                "user_id": {
                    "type": "varchar",
                    "length": 100,
                    "comment": "OAuth 유저고유값"
                },
                "scope": {
                    "type": "text",
                    "is_null": true,
                    "comment": "OAuth 요청범위"
                },
                "access_token": {
                    "type": "text",
                    "comment": "OAuth 엑세스토큰"
                },
                "access_token_expired_at": {
                    "type": "int",
                    "length": 11,
                    "default": 0,
                    "comment": "엑세스토큰만료일시"
                },
                "refresh_token": {
                    "type": "text",
                    "is_null": true,
                    "comment": "OAuth 리프레시토큰"
                },
                "latest_access": {
                    "type": "int",
                    "length": 11,
                    "default": 0,
                    "comment": "최종활성화시각"
                }
            },
            "indexes": {
                "oauth_id,member_id,user_id": "primary_key"
            },
            "comment": "아이모듈 OAuth 토큰"
        }
    }
}
